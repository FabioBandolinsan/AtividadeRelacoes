
public class Tinta {

	private String cor;

	public Tinta(String cor) {
		setCor(cor);
	}
	
	public String getCor() {
		return cor;
	}

	public void setCor(String cor) {
		if(cor.length()>0)
			this.cor = cor;
	}

	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("Tinta [cor=");
		builder.append(cor);
		builder.append("]");
		return builder.toString();
	}
	
	
}
